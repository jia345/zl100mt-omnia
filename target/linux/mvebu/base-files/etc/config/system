config system
        option hostname 'zl100mt-100'
        option timezone 'GMT-8'
        option zonename 'Asia/Shanghai'
        option cronloglevel '8'
        option conloglevel '4'

config timeserver ntp
        list server     '210.72.145.44'
        option enabled 1
        option enable_server '0'

